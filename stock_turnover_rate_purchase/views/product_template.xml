<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="product_template_form_with_minimum_turnover_rate" model="ir.ui.view">
        <field name="name">Product Template Form: Add minimum turnover rate</field>
        <field name="model">product.template</field>
        <field name="inherit_id" ref="stock_turnover_rate.product_template_form_with_turnover_fields"/>
        <field name="arch" type="xml">
            <field name="target_turnover_rate" position="after">
                <field name="minimum_turnover_rate"/>
            </field>
        </field>
    </record>

</odoo>
