<?xml version="1.0" encoding="UTF-8"?>
<odoo>

    <record id="product_category_form_with_target_turnover" model="ir.ui.view">
        <field name="name">Product Category Form: Add minimum turnover rate</field>
        <field name="model">product.category</field>
        <field name="inherit_id" ref="stock_turnover_rate.product_category_form_with_target_turnover"/>
        <field name="arch" type="xml">
            <field name="target_turnover_rate" position="after">
                <field name="minimum_turnover_rate"/>
            </field>
        </field>
    </record>

</odoo>
